{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "id": {
        "type": "integer"
      },

      "year": {
        "type": "integer"
      },

      "name": {
        "type": "string"
      },

      "current": {
        "type": "boolean"
      }
    },
    "required": ["id", "year", "name", "current"]
  }
}